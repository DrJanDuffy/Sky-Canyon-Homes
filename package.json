{
  "name": "rest-express",
  "version": "1.0.0",
  "description": "Modern full-stack React + Express app with TypeScript, Tailwind, and AI integration",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=10.0.0"
  },
  "packageManager": "npm@11.4.2",
  "scripts": {
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "NODE_ENV=development tsx watch server/index.ts",
    "dev:client": "vite --host",
    "build": "npm run build:client && npm run build:server",
    "build:client": "vite build",
    "build:server": "node build-esbuild.js",
    "build:esbuild": "node build-esbuild.js",
    "start": "NODE_ENV=production node dist/index.js",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:check": "eslint . --ext .ts,.tsx,.js,.jsx",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md,css}\"",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "db:generate": "drizzle-kit generate",
    "db:push": "drizzle-kit push",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "clean": "rm -rf dist && rm -rf .next && rm -rf node_modules/.cache",
    "clean:install": "npm run clean && npm install",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "deps:update": "npx npm-check-updates -u",
    "deps:check": "npx npm-check-updates",
    "analyze": "npx vite-bundle-analyzer",
    "prepare": "husky install || true"
  },
